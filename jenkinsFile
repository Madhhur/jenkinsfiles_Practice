@Library('My_Shared') _

pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
               script {
    // Call your shared library method
    def config = [
        url: 'https://github.com/Madhhur/repo1.git',
        branch: 'main',
        credentialId: 'Madhhur'
    ]
    
    gitCheckout(config)

    sh '''
        echo "Checkout completed"
        pwd
        ls -lrt
    '''
}

            }
        }
    }
}
