@Library('my-shared') _

pipeline{
    agent{
        label "master"
    }
    stages{
        stage("first"){
          steps {
                script {
                    gitCheckout([
                        url: 'https://github.com/Madhhur/jenkinsfiles_Practice.git',
                        branch: 'main',
                        credentialId: 'Madhhur'
                    ])
                }
           sh '''
           pwd
           ls -lrta
           '''
            }   
        }
    }
    post{
        always{
            echo "========always========"
        }
        success{
            echo "========pipeline executed successfully ========"
        }
        failure{
            echo "========pipeline execution failed========"
        }
    }
}   