@Library('My_Shared@main') _

pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                script {
                    // Run trusted library function without sandbox restriction
                    gitCheckout([
                        url: 'https://github.com/Madhhur/repo1.git',
                        branch: 'main',
                        credentialId: 'Madhhur'
                    ])
                }
            }
        }
    }
}
